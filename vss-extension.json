{
    "manifestVersion": 1,
    "id": "version-checker",
    "publisher": "Neocles",
    "version": "1.0.16",
    "name": "Version Checker",
    "description": "Azure DevOps Extension",
    "public": false,
    "categories": [
        "Azure Repos",
        "Azure Boards",
        "Azure Pipelines",
        "Azure Test Plans",
        "Azure Artifacts"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "icons": {
        "default": "static/img/world.png"
    },
    "contributions": [
        {
            "id": "butler-hub-group",
            "type": "ms.vss-web.hub-group",
            "description": "Butler Hub",
            "targets": [
                "ms.vss-web.project-hub-groups-collection"
            ],
            "properties": {
                "name": "Multi-tenant Hub",
                "icon": {
                    "dark": "static/img/world.png",
                    "light": "static/img/world.png"
                },
                "supportsMobile": true
            }
        },
        {
            "id": "default-hub",
            "type": "ms.vss-web.hub",
            "targets": [
                ".butler-hub-group"
            ],
            "properties": {
                "name": "Versions",
                "uri": "dist/index/index.html",
                "supportsMobile": true
            }
        }
    ],
    "files": [
        {
            "path": "dist",
            "addressable": true
        },
        {
            "path": "static",
            "addressable": true
        }
    ]
}