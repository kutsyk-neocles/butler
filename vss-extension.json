{
    "manifestVersion": 1,
    "id": "version-checker",
    "publisher": "Neocles",
    "version": "1.0.8",
    "name": "Version Checker",
    "description": "Azure DevOps Extension",
    "public": false,
    "categories": [
        "Azure Repos",
        "Azure Boards",
        "Azure Pipelines",
        "Azure Test Plans",
        "Azure Artifacts"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "icons": {
        "default": "static/img/world.png"
    },
    "contributions": [
        {
            "id": "hello-world-hub-group",
            "type": "ms.vss-web.hub-group",
            "description": "Sample hub group",
            "targets": [
                "ms.vss-web.project-hub-groups-collection"
            ],
            "properties": {
                "name": "Butler",
                "icon": {
                    "dark": "static/img/world.png",
                    "light": "static/img/world.png"
                }
            }
        },
        {
            "id": "default-hub",
            "type": "ms.vss-web.hub",
            "targets": [
                ".hello-world-hub-group"
            ],
            "properties": {
                "uri": "dist/hub/hub.html"
            }
        }
    ],
    "files": [
        {
            "path": "dist",
            "addressable": true
        },
        {
            "path": "static",
            "addressable": true
        }
    ]
}